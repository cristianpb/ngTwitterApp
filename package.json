    "@types/node": "^10.1.3",
    "@types/supertest": "^2.0.4",
    "@types/twit": "^2.2.19",
    "angular2-moment": "^1.9.0",
    "bulma": "^0.7.1",
    "colors": "^1.3.2",
    "concurrently": "^4.0.1",
    "core-js": "^2.5.4",
